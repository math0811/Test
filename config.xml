<?xml version='1.0' encoding='utf-8'?>
<widget id="sup.brahs.link" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">

    <name>Mathias-Test</name>
    <description>
        Test af InAppBrowser
    </description>
    <author >
         Lone Ã˜sterlund based on an example by Don Coleman: 
		 https://github.com/don/cordova-plugin-ble-central 
		 revideret til IoS af Rico Ghanem
    </author>

    <content src="index.html" />
	
	<plugin name="cordova-plugin-ble-central" spec="^1.1.4" />
    
	<plugin name="cordova-plugin-inappbrowser" spec="~2.0.1" />
	<preference name="phonegap-version" value="cli-6.5.0" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
